  "quadrantRules": {
    "description": "ASTC rule for remembering which trig functions are positive in each quadrant",
    "mnemonic": "All Students Take Calculus",
    "rules": {
      "Q1": {
        "angles": "0° to 90° (0 to π/2)",
        "positive": ["sin", "cos", "tan", "csc", "sec", "cot"],
        "memory": "ALL positive (first letter A)"
      },
      "Q2": {
        "angles": "90° to 180° (π/2 to π)",
        "positive": ["sin", "csc"],
        "negative": ["cos", "tan", "sec", "cot"],
        "memory": "SINE positive (first letter S)"
      },
      "Q3": {
        "angles": "180° to 270° (π to 3π/2)",
        "positive": ["tan", "cot"],
        "negative": ["sin", "cos", "csc", "sec"],
        "memory": "TANGENT positive (first letter T)"
      },
      "Q4": {
        "angles": "270° to 360° (3π/2 to 2π)",
        "positive": ["cos", "sec"],
        "negative": ["sin", "tan", "csc", "cot"],
        "memory": "COSINE positive (first letter C)"
      }
    }
  },
  "mnemonics": {
    "sohcahtoa": {
      "description": "Mnemonic for right triangle trig ratios",
      "expansions": {
        "SOH": "Sine = Opposite / Hypotenuse",
        "CAH": "Cosine = Adjacent / Hypotenuse",
        "TOA": "Tangent = Opposite / Adjacent"
      }
    },
    "specialAngles": {
      "description": "Memorizing special angle values",
      "method": "0-1-2-3-4 pattern divided by 2 under square root",
      "pattern": {
        "0°": "sin(0°) = √0/2 = 0",
        "30°": "sin(30°) = √1/2 = 1/2",
        "45°": "sin(45°) = √2/2",
        "60°": "sin(60°) = √3/2",
        "90°": "sin(90°) = √4/2 = 1"
      },
      "note": "Cosine uses the same pattern in reverse (90° to 0°)"
    },
    "referenceAngles": {
      "description": "Finding reference angles in each quadrant",
      "formulas": {
        "Q1": "Reference angle = θ (the angle itself)",
        "Q2": "Reference angle = π - θ (or 180° - θ)",
        "Q3": "Reference angle = θ - π (or θ - 180°)",
        "Q4": "Reference angle = 2π - θ (or 360° - θ)"
      }
    },
    "degreeRadianConversion": {
      "description": "Converting between degrees and radians",
      "formulas": {
        "toRadians": "radians = degrees × (π/180)",
        "toDegrees": "degrees = radians × (180/π)"
      },
      "keyValues": {
        "30°": "π/6",
        "45°": "π/4",
        "60°": "π/3",
        "90°": "π/2",
        "180°": "π",
        "360°": "2π"
      }
    }
  },
